{
    "version": "2.0.0",    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/RVToolsMerge.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "run-standard",
            "command": "dotnet",
            "type": "process",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/RVToolsMerge.csproj"
            ],
            "problemMatcher": "$msCompile",
            "dependsOn": ["build"]
        },
        {
            "label": "run-with-mandatory-columns",
            "command": "dotnet",
            "type": "process",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/RVToolsMerge.csproj",
                "--",
                "-o",
                "${workspaceFolder}/input",
                "${workspaceFolder}/RVTools_mandatory.xlsx"
            ],
            "problemMatcher": "$msCompile",
            "dependsOn": ["build"]
        }
    ]
}
